/* 地图 */
.showAddr{
  position: fixed;
  left: 50%;
  top: 42%;
  height:132rpx;
  width:442rpx;
  margin-left: -220rpx;
  margin-top: -68rpx;
  justify-content: space-between;
}
.center_icon{
  height:72rpx;
  width:72rpx;
}
.showAddrInfo{
  width: 220px;
  line-height: 54rpx;
  background: rgba(0, 0, 0, 0.7);
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: center;
  /* position: absolute; */
  /* top: 0rpx; */
  /* border-radius: 54rpx; */
  color: #fff;
  font-size: 24rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.initAn{
  height: 100%;
  width: 100%;
  background: #fff;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 100;
  justify-content: center;
  align-items: center;
  opacity: 1;
}
.initAn text{
  margin-top: 20rpx;
  font-size: 26rpx;
}
.title{
  line-height:2.8;
  padding:0 25rpx;
  margin-bottom: 15rpx;
  
}
.title>view{
    color:#606266;
    font-weight: bold;
    border-bottom: 2px solid #DFDFDF;
    position: relative;
    padding-left:30rpx;
    justify-content: space-between;
}
.title>view text:last-child{
  font-weight: normal;
  color: #909399;
}
.title>view::after{
  height: 30rpx;
  width:8rpx;
  background: #606266;
  content: '';
  position: absolute;
  left: 10rpx;
  top: 21rpx;
}