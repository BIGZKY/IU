.navClass{
  width: 80%;
  background: #fff;
  height: 65rpx;
  justify-content: space-around;
  margin:15px auto;
  /* box-shadow: 0px 0px 8rpx 2rpx #ff5703; */
} 
.navClass view{
  flex: 1 0 33.333%;
  text-align: center;
  box-sizing: border-box;
  line-height: 65rpx;
  color:#555;
  border:3rpx solid #06BEBC;
  background: #fff
}
.navClass view:nth-child(2){
  border-left: none;
  border-right: none;
}
.navClass view.navOn{
  color: #fff;
  background: #06BEBC;
}


.deliveryWrap{
  box-sizing: border-box;
  padding: 0 25rpx;
  margin-bottom:25rpx;

}
.delivery{
  width: 100%;
  margin-top: 30rpx;
  box-shadow: 0 0rpx 15rpx 6rpx #e2e2e2;
  padding:20rpx;
  box-sizing: border-box;
  padding-bottom:0rpx;
}
.orderWrap{
  width: 100%;
  border-bottom:1px solid #e2e2e2;
  border-top:1px solid #e2e2e2;
  padding-bottom: 15rpx;
  padding-top: 15rpx;
}
.snInfo{
  width: 100%;
  padding-bottom:20rpx;

}
.userInfo{
  height: 110px;
  width: 90px;
}
.userInfo image{
  height:65px;
  width:65px;
}
.orderInfo{
  /* height: 90px; */
  justify-content: space-around;
  align-items: flex-start;
  flex: 1
}
.infoItem{
  width: 100%;
  align-items: flex-start;
  margin-bottom:25rpx;

}
.info{
  align-items: flex-end;
  flex: 1;
}
.userInfo text{
  margin-top: 10rpx;
}
.receiveOrder{
  width: 100%;
  height:45px;
  justify-content: flex-end;
}
.receiveOrder text{
  display:inline-block;
  background:#06BEBC;
  height:32px;
  width:60px;
  text-align:center;
  line-height:32px;
  color:#fff;
  /* margin-right:25rpx; */
  border-radius:12rpx;

}
.info text{
  color: #888;
}
.info>text{ 
  margin-top: 10rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.info>view text:last-child{
  margin-left: 20rpx;
}