.pruduct-list{
  margin-top: 10rpx;
  justify-content:space-between;

}
.product{
  width: 60px;
}

.product .name>text{
  font-size:24rpx;
  padding:4rpx 6rpx;
  color:#555;
  line-height:1.1;
  display:block;
  white-space:nowrap;
  margin-top: 2px;
  text-overflow: ellipsis;
  text-align: center;
}
.product-img{
  height: 60px;
  width: 60px;
  display: block;
  background: #f2f2f2;
  /* background: red; */
}
.product-img image{
  height: 100%;
  width: 100%;
}

/* footer */
.footers{
  background: #fff;
  justify-content: space-around;
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  border-top:1px solid #f2f2f2;
  box-sizing: border-box;
  height: 65px
}
.footers text{
  margin-top: 8rpx;
  font-size: 26rpx;
}
.footers image{ 
  height:56rpx;
  width: 56rpx;

}
.secondNav{
  height: 100%;
  position: absolute;
  width: 100%;
  background: #fff;
  left: 100%;
  top: 0;

}
.secondNav>.item{
  width: 40%;
}
.secondNav>.item:last-child{
  width: 20%;
  height:100%;
  justify-content:center;

}  
.secondNav>.item:last-child image{
  transform: rotate(180deg)
}

/* 首页代跑卷 */
.voucher {
  height:236px;
  width:290px;
  position:relative;
  background:#F1525A;
  border-radius:10rpx;

}

.dpjWrap{
  height:320px;
  width:290px;
  position:fixed;
  left:50%;
  top:50%;
  margin-left:-145px;
  margin-top:-160px;
  z-index:1000;
  border-radius:10rpx;

}
.close{
  bottom:-22px;
}
button{
  background: #fff;

}
button::after{
  border: none
}

