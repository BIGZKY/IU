.container,page{
  height: 100%;
  overflow: hidden;
  background: #f2f2f2;
}
.container>view{
  width: 100%;
}
.news{
  border-radius:5rpx;
  width:100%;
  height:75rpx;
  background:#06bebc;
  box-sizing:border-box;
  padding:0 30rpx;
  overflow: hidden;
  flex: 0 0 37px;
  justify-content:space-between;

}
.news-img{
  float: left;
  color: #fff;
  font-weight: 600;
  position: relative;
  height: 100%;
  line-height: 75rpx;
  
}
.news-img::after{
  content: '';
  position: absolute;
  right: -17px;
  top: -10px;
  height: 150%;
  width: 2px;
  background: #fff;
  transform: rotate(22deg)
}

.news .text{
  line-height: 75rpx;
  text-align: center;
  width: 80%;
  height: 75rpx;
  display: inline-block;
  color: #9d9d9e;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.word{
  overflow: hidden;
  text-overflow: ellipsis;
  box-sizing: border-box;
  padding-left: 25rpx;
  color: #fff;
}

.banner{
  flex: 0 0 100px;
}
/* main */
.main{
  /* background: rgb(228, 228, 230); */
  flex: 1;
  position: relative;
  overflow: scroll;
  box-sizing: border-box;
  padding-bottom: 65px;
}
.main .haibao{
  height: 100%;
  width: 100%;
}
.main .haibao image{
  height: 100%;
  width: 100%;
}
.main.active{
  height: 100%;
  position: absolute;
  top: 107px;
  width: 100%;
  background: #fff;
}
.main.active>view{
  height: 100%;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  background: transparent;
}
.shop-pic{
  height:100rpx;
  width:100rpx;
  border-radius: 50%;
  background: rgb(226, 225, 225);
  border:2px solid #fff;
  overflow: hidden
}
.ww .shop-pic{
  border-radius:0; 
}
.shop-pic image{
  height:100%;
  width:100%;
}
.shop-title{
  font-size: 12px;
  color: #555;
  display:block;
  white-space:nowrap;
  text-align:center;
  width: 95%;
  text-overflow: ellipsis;
}
.shopList{
  padding: 0rpx 25rpx;
  justify-content: flex-start;
  flex-wrap: wrap;
  align-items:flex-start;
  padding-bottom: 10rpx;
}
.shop-item{ 
  align-items: center;
  margin-bottom:4px;
  width:20%;
}
.moreShop{
  margin-top: 12px;
}
.moreShop .shop-pic{
  justify-content: space-between;
  flex-wrap:wrap;

}
.moreShop .shop-pic{
  background: none;
}
